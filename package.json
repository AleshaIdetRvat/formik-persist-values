{
  "name": "formik-persist-values",
  "description": "Persist and rehydrate a Formik values",
  "version": "1.0.0",
  "license": "MIT",
  "author": "Grigoriy Kolenko <kolenko@me.com>",
  "repository": "kolengri/formik-persist-values",
  "keywords": [
    "formik",
    "react",
    "react-dom",
    "form",
    "validation",
    "forms",
    "localstorage",
    "hoc"
  ],
  "main": "dist/PersistFormikValues.js",
  "module": "dist/PersistFormikValues.es6.js",
  "typings": "dist/PersistFormikValues.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest --env=jsdom",
    "test:watch": "npm run test -- --watch",
    "start": "cross-env NODE_ENV=development tsc-watch --onSuccess \"rollup -c\"",
    "prebuild": "rimraf dist",
    "build": "tsc && cross-env NODE_ENV=production rollup -c && rimraf compiled",
    "prepublish": "npm run build",
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/**/*' 'test/**/*'",
    "precommit": "lint-staged",
    "addc": "all-contributors add",
    "gen-docs": "all-contributors generate && doctoc README.md"
  },
  "dependencies": {
    "react-use": "^13.14.3"
  },
  "peerDependencies": {
    "formik": ">=2.0.0",
    "react": ">=16"
  },
  "resolutions": {
    "@types/react": "16.9.17",
    "@types/react-dom": "16.0.6"
  },
  "optionalDependencies": {},
  "devDependencies": {
    "@types/jest": "20.0.6",
    "@types/lodash.debounce": "^4.0.3",
    "@types/node": "8.0.19",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "all-contributors-cli": "^4.4.0",
    "cross-env": "5.0.5",
    "doctoc": "^1.3.0",
    "formik": "^2.1.1",
    "husky": "0.14.3",
    "jest": "20.0.4",
    "lint-staged": "4.0.2",
    "prettier": "1.19.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "rimraf": "2.6.1",
    "rollup": "0.45.2",
    "rollup-plugin-commonjs": "8.1.0",
    "rollup-plugin-filesize": "1.4.2",
    "rollup-plugin-node-resolve": "3.0.0",
    "rollup-plugin-replace": "1.1.1",
    "rollup-plugin-sourcemaps": "0.4.2",
    "rollup-plugin-uglify": "2.0.1",
    "ts-jest": "20.0.9",
    "tsc-watch": "4.0.0",
    "tslint": "^5.20.1",
    "tslint-react": "4.1.0",
    "typescript": "^3.7.4"
  },
  "lint-staged": {
    "**/*.{ts,tsx}": [
      "prettier --trailing-comma es5 --single-quote --write",
      "git add"
    ]
  },
  "prettier": {
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": true
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testMatch": [
      "<rootDir>/test/**/*.ts?(x)",
      "<rootDir>/test/**/?(*.)(spec|test).ts?(x)"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
